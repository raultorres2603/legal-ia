meta {
  name: Get Form Guidance - Invalid Form Type
  type: http
  seq: 14
}

post {
  url: {{baseUrl}}/api/ai/legal/form-guidance
  body: json
  auth: none
}

body:json {
  {
    "formType": "999",
    "quarter": "Q2",
    "year": 2025,
    "regimeType": "general"
  }
}

tests {
  test("Status should be 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Should handle invalid form type gracefully", function() {
    const responseJson = res.getBody();
    expect(responseJson.formType).to.equal("999");
    expect(responseJson.success).to.be.true;
    expect(responseJson.instructions).to.be.a('string');
  });
}
