meta {
  name: Get Annual Obligations - Personalized
  type: http
  seq: 10
}

get {
  url: {{baseUrl}}/api/ai/legal/annual-obligations/2025?userId={{userId}}
  body: none
  auth: none
}

tests {
  test("Status should be 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response should contain personalized annual obligations", function() {
    const responseJson = res.getBody();
    expect(responseJson.obligations).to.be.a('string');
    expect(responseJson.obligations).to.contain("2025");
    expect(responseJson.obligations).to.contain("anual");
    // Should contain personalized amounts or user-specific information
    expect(responseJson.obligations.length).to.be.greaterThan(200);
  });
}
