meta {
  name: Create Invoice Item By Current User
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/invoice-items/user
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "InvoiceId": {{invoiceId}},
    "Description": "Consulting service",
    "Quantity": 1,
    "UnitPrice": 500.00,
    "VAT": 105.00,
    "IRPF": 75.00,
    "Total": 530.00
  }
}

settings {
  encodeUrl: true
}
