meta {
  name: Update Invoice Item
  type: http
  seq: 2
}

put {
  url: {{baseUrl}}/invoice-items/users/{{invoiceItemId}}
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "Id": "{{invoiceItemId}}",
    "InvoiceId": "{{invoiceId}}",
    "Description": "Consulting service updated",
    "Quantity": 2,
    "UnitPrice": 500.00,
    "VAT": 210.00,
    "IRPF": 150.00,
    "Total": 1060.00
  }
}

settings {
  encodeUrl: true
}
